#include <stdio.h>
#include <locale.h>
#include<stdlib.h>
 
int main (void)
{
	int a, b, c;
	printf("Vvedite 3 chicla cherez probel: ");
	scanf("%d %d %d", &a, &b, &c );
	
	if ((a > b)&&(a > c)) {
	printf("\nNaibolshee chislo: %d", a);
	} else {
	if ((b > a)&&(b > c)) {
	printf("\nNaibolshee chislo: %d", b);
	} else {
	if ((c > a)&&(c > b)) {
	printf("\nNaibolshee chislo: %d", c);
	} else {}
	}
	}
	system("pause");
    return 0;
}

