#include <stdio.h>
#include <math.h>
#include <stdlib.h>

int main(int argc, char *argv[]) {
	double a,b,x;
	printf("Reshatel uravneniy vida: a*x+b=0 \n");
	
	printf("Vvedite a:");
	scanf("%f", &a);
	printf("Vvedite b:");
	scanf("%f", &b);
	
	x = - b / a ;
	
	printf("x=%f", x); 
	system("pause");
	return 0;
}
